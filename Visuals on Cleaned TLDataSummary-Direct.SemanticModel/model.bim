              "",
              "VAR __CORRELATION_TABLE = VALUES('df_model_data'[CLUSTER])",
              "VAR __COUNT =",
              "\tCOUNTX(",
              "\t\tKEEPFILTERS(__CORRELATION_TABLE),",
              "\t\tCALCULATE(",
              "\t\t\tAVERAGE('df_model_data'[Num_Months_Since_Start])",
              "\t\t\t\t* AVERAGE('df_model_data'[Total_Revenue])",
              "\t\t)",
              "\t)",
              "VAR __SUM_X =",
              "\tSUMX(",
              "\t\tKEEPFILTERS(__CORRELATION_TABLE),",
              "\t\tCALCULATE(AVERAGE('df_model_data'[Num_Months_Since_Start]))",
              "\t)",
              "VAR __SUM_Y =",
              "\tSUMX(",
              "\t\tKEEPFILTERS(__CORRELATION_TABLE),",
              "\t\tCALCULATE(AVERAGE('df_model_data'[Total_Revenue]))",
              "\t)",
              "VAR __SUM_XY =",
              "\tSUMX(",
              "\t\tKEEPFILTERS(__CORRELATION_TABLE),",
              "\t\tCALCULATE(",
              "\t\t\tAVERAGE('df_model_data'[Num_Months_Since_Start])",
              "\t\t\t\t* AVERAGE('df_model_data'[Total_Revenue]) * 1.",
              "\t\t)",
              "\t)",
              "VAR __SUM_X2 =",
              "\tSUMX(",
              "\t\tKEEPFILTERS(__CORRELATION_TABLE),",
              "\t\tCALCULATE(AVERAGE('df_model_data'[Num_Months_Since_Start]) ^ 2)",
              "\t)",
              "VAR __SUM_Y2 =",
              "\tSUMX(",
              "\t\tKEEPFILTERS(__CORRELATION_TABLE),",
              "\t\tCALCULATE(AVERAGE('df_model_data'[Total_Revenue]) ^ 2)",
              "\t)",
              "RETURN",
              "\tDIVIDE(",
              "\t\t__COUNT * __SUM_XY - __SUM_X * __SUM_Y * 1.,",
              "\t\tSQRT(",
              "\t\t\t(__COUNT * __SUM_X2 - __SUM_X ^ 2)",
              "\t\t\t\t* (__COUNT * __SUM_Y2 - __SUM_Y ^ 2)",
              "\t\t)",
              "\t)"
            ],
