          {
            "name": "isWithinThreshold",
            "expression": [
              "",
              "IF(",
              "    'TLDataSummary-Cleaned'[Distance(KM)] <= 'Maximum Distance'[Maximum Distance Value],",
              "    1,",
              "    0",
              ")"
            ],
            "formatString": "0",
            "lineageTag": "45b163ab-ad06-41d4-9e3c-e92843479223"
          },
          {
            "name": "Distance(KM)",
            "expression": [
              "",
              "Var lat1 = MIN('USA States Geo'[Latitude])",
              "Var lng1 = MIN('USA States Geo'[Longitude])",
              "",
              "Var lat2 = MIN('TLDataSummary-Cleaned'[zip_lat])",
              "Var lng2 = MIN('TLDataSummary-Cleaned'[zip_long])",
              "",
              "Var P = DIVIDE(PI(), 180)",
              "Var A = 0.5 - COS((lat2 - lat1) * P)/2 +",
              "        COS(lat1 * P) * COS(lat2 * P) * (1 - COS((lng2 - lng1) * P))/2",
              "Var final = ROUND(12742 * ASIN((SQRT(A))),0)",
              "Return",
              "final"
            ],
            "formatString": "0",
